  contentFooter             Footer?               @relation("Section_contentFooter", fields: [contentFooterId], references: [id])
  contentFooterId           Int?                  @map("contentFooter")
  contentCta                CtaSection?           @relation("Section_contentCta", fields: [contentCtaId], references: [id])
  contentCtaId              Int?                  @map("contentCta")
  contentMap                Map?                  @relation("Section_contentMap", fields: [contentMapId], references: [id])
  contentMapId              Int?                  @map("contentMap")
  from_PageContent_sections PageContent[]         @relation("PageContent_sections")

  @@index([contentHeroId])
  @@index([contentBenefitsId])
  @@index([contentTestimonialsId])
  @@index([contentCertificationsId])
  @@index([contentApproachId])
  @@index([contentAboutId])
  @@index([contentAnalyticsId])
  @@index([contentNavigationId])
  @@index([contentFooterId])
