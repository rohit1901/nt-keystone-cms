  contentTestimonials       TestimonialSection?   @relation("Section_contentTestimonials", fields: [contentTestimonialsId], references: [id])
  contentTestimonialsId     Int?                  @map("contentTestimonials")
  contentCertifications     CertificationSection? @relation("Section_contentCertifications", fields: [contentCertificationsId], references: [id])
  contentCertificationsId   Int?                  @map("contentCertifications")
  contentApproach           Approach?             @relation("Section_contentApproach", fields: [contentApproachId], references: [id])
  contentApproachId         Int?                  @map("contentApproach")
  contentAbout              About?                @relation("Section_contentAbout", fields: [contentAboutId], references: [id])
  contentAboutId            Int?                  @map("contentAbout")
  contentAnalytics          Analytic?             @relation("Section_contentAnalytics", fields: [contentAnalyticsId], references: [id])
