}

model Navigation {
  id                             Int              @id @default(autoincrement())
  title                          String           @default("")
  description                    String           @default("")
  image                          Image?           @relation("Navigation_image", fields: [imageId], references: [id])
  imageId                        Int?             @map("image")
  cta                            Cta?             @relation("Navigation_cta", fields: [ctaId], references: [id])
  ctaId                          Int?             @map("cta")
  items                          NavigationLink[] @relation("Navigation_items")
  from_Section_contentNavigation Section[]        @relation("Section_contentNavigation")

  @@index([imageId])
  @@index([ctaId])
}

model FooterSection {
  id                   Int              @id @default(autoincrement())
  title                String           @default("")
  items                NavigationLink[] @relation("FooterSection_items")
  from_Footer_sections Footer[]         @relation("Footer_sections")
}

model Footer {
  id                         Int             @id @default(autoincrement())
  title                      String          @default("")
  sections                   FooterSection[] @relation("Footer_sections")
  languages                  Language[]      @relation("Footer_languages")
  from_Section_contentFooter Section[]       @relation("Section_contentFooter")
}

model AnalyticsStat {
  id                      Int        @id @default(autoincrement())
  totalDeployments        String     @default("")
  deploymentChange        String     @default("")
  deploymentChangePercent String     @default("")
  changePeriod            String     @default("")
  from_Analytic_stats     Analytic[] @relation("Analytic_stats")
}

model AnalyticsSummaryItem {
  id                    Int        @id @default(autoincrement())
  name                  String     @default("")
  deployments           String     @default("")
  uptime                String     @default("")
  clientSatisfaction    String     @default("")
  efficiency            String     @default("")
  revenueGrowth         String     @default("")
  bgColor               String     @default("")
  changeType            String
  from_Analytic_summary Analytic[] @relation("Analytic_summary")
}

model Analytic {
  id                            Int                    @id @default(autoincrement())
  heading                       String                 @default("")
  subheading                    String                 @default("")
  stats                         AnalyticsStat?         @relation("Analytic_stats", fields: [statsId], references: [id])
  statsId                       Int?                   @map("stats")
