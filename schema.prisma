// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource sqlite {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "sqlite"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String    @id @default(cuid())
  name      String    @default("")
  email     String    @unique @default("")
  password  String
  createdAt DateTime? @default(now())
}

model Cta {
  id                    String        @id @default(cuid())
  label                 String        @default("")
  href                  String        @default("")
  external              Boolean       @default(false)
  from_CtaSection_ctas  CtaSection[]  @relation("CtaSection_ctas")
  from_FooterPart_items FooterPart[]  @relation("FooterPart_items")
  from_PageContent_cta  PageContent[] @relation("PageContent_cta")
}

model CtaSection {
  id                      String       @id @default(cuid())
  background              Background[] @relation("CtaSection_background")
  ctas                    Cta[]        @relation("CtaSection_ctas")
  from_Section_contentCta Section[]    @relation("Section_contentCta")
}

model Image {
  id                       String          @id @default(cuid())
  width                    Int?
  height                   Int?
  src                      String          @default("")
  alt                      String          @default("")
  fill                     Boolean         @default(false)
  className                String          @default("")
  from_Background_image    Background[]    @relation("Background_image")
  from_Testimonial_image   Testimonial[]   @relation("Testimonial_image")
  from_Certification_image Certification[] @relation("Certification_image")
  from_PageContent_image   PageContent[]   @relation("PageContent_image")
}

model Background {
  id                                 String               @id @default(cuid())
  image                              Image?               @relation("Background_image", fields: [imageId], references: [id])
  imageId                            String?              @map("image")
  outerClassName                     String               @default("")
  from_CtaSection_background         CtaSection[]         @relation("CtaSection_background")
  from_TestimonialSection_background TestimonialSection[] @relation("TestimonialSection_background")

  @@index([imageId])
}

model TestimonialBadge {
  id                     String        @id @default(cuid())
  icon                   String        @default("")
  label                  String        @default("")
  from_Testimonial_badge Testimonial[] @relation("Testimonial_badge")
}

model Testimonial {
  id                                   String               @id @default(cuid())
  badge                                TestimonialBadge?    @relation("Testimonial_badge", fields: [badgeId], references: [id])
  badgeId                              String?              @map("badge")
  name                                 String               @default("")
  role                                 String               @default("")
  company                              String               @default("")
  image                                Image?               @relation("Testimonial_image", fields: [imageId], references: [id])
  imageId                              String?              @map("image")
  content                              String               @default("")
  from_TestimonialSection_testimonials TestimonialSection[] @relation("TestimonialSection_testimonials")
  from_TestimonialSection_fallback     TestimonialSection[] @relation("TestimonialSection_fallback")

  @@index([badgeId])
  @@index([imageId])
}

model TestimonialSection {
  id                               String        @id @default(cuid())
  background                       Background[]  @relation("TestimonialSection_background")
  testimonials                     Testimonial[] @relation("TestimonialSection_testimonials")
  fallback                         Testimonial?  @relation("TestimonialSection_fallback", fields: [fallbackId], references: [id])
  fallbackId                       String?       @map("fallback")
  from_Section_contentTestimonials Section[]     @relation("Section_contentTestimonials")

  @@index([fallbackId])
}

model HeroBannerAdditional {
  id                         String       @id @default(cuid())
  icon                       String       @default("")
  text                       String       @default("")
  from_HeroBanner_additional HeroBanner[] @relation("HeroBanner_additional")
}

model HeroBanner {
  id               String                @id @default(cuid())
  label            String                @default("")
  href             String                @default("")
  external         Boolean               @default(false)
  additional       HeroBannerAdditional? @relation("HeroBanner_additional", fields: [additionalId], references: [id])
  additionalId     String?               @map("additional")
  from_Hero_banner Hero[]                @relation("Hero_banner")

  @@index([additionalId])
}

model Hero {
  id                       String      @id @default(cuid())
  subHeading               String      @default("")
  banner                   HeroBanner? @relation("Hero_banner", fields: [bannerId], references: [id])
  bannerId                 String?     @map("banner")
  from_Section_contentHero Section[]   @relation("Section_contentHero")

  @@index([bannerId])
}

model Benefit {
  id                           String    @id @default(cuid())
  icon                         String    @default("")
  title                        String    @default("")
  description                  String    @default("")
  from_Section_contentBenefits Section[] @relation("Section_contentBenefits")
}

model Faq {
  id                       String    @id @default(cuid())
  question                 String    @default("")
  answer                   String    @default("")
  from_Section_contentFaqs Section[] @relation("Section_contentFaqs")
}

model Certification {
  id                                 String    @id @default(cuid())
  title                              String    @default("")
  description                        String    @default("")
  image                              Image?    @relation("Certification_image", fields: [imageId], references: [id])
  imageId                            String?   @map("image")
  link                               String    @default("")
  from_Section_contentCertifications Section[] @relation("Section_contentCertifications")

  @@index([imageId])
}

model Feature {
  id                           String    @id @default(cuid())
  fid                          String    @default("")
  title                        String    @default("")
  description                  String    @default("")
  longDescription              String    @default("")
  visualization                String    @default("")
  from_Section_contentFeatures Section[] @relation("Section_contentFeatures")
}

model ApproachStep {
  id                  String     @id @default(cuid())
  type                String?
  title               String     @default("")
  description         String     @default("")
  activityTime        String     @default("")
  from_Approach_steps Approach[] @relation("Approach_steps")
}

model Approach {
  id                           String         @id @default(cuid())
  title                        String         @default("")
  description                  String         @default("")
  steps                        ApproachStep[] @relation("Approach_steps")
  from_Section_contentApproach Section[]      @relation("Section_contentApproach")
}

model Language {
  id    String  @id @default(cuid())
  label String?
  value String?
}

model NavigationLink {
  id                             String    @id @default(cuid())
  label                          String    @default("")
  href                           String    @default("")
  external                       Boolean   @default(false)
  icon                           String    @default("")
  from_Section_contentNavigation Section[] @relation("Section_contentNavigation")
}

model FooterPart {
  id                   String   @id @default(cuid())
  title                String   @default("")
  items                Cta[]    @relation("FooterPart_items")
  from_Footer_sections Footer[] @relation("Footer_sections")
}

model Footer {
  id                         String       @id @default(cuid())
  sections                   FooterPart[] @relation("Footer_sections")
  from_Section_contentFooter Section[]    @relation("Section_contentFooter")
}

model AnalyticsStat {
  id                          String             @id @default(cuid())
  totalDeployments            String             @default("")
  deploymentChange            String             @default("")
  deploymentChangePercent     String             @default("")
  changePeriod                String             @default("")
  from_AnalyticsSection_stats AnalyticsSection[] @relation("AnalyticsSection_stats")
}

model AnalyticsSummaryItem {
  id                            String             @id @default(cuid())
  name                          String             @default("")
  deployments                   String             @default("")
  uptime                        String             @default("")
  clientSatisfaction            String             @default("")
  efficiency                    String             @default("")
  revenueGrowth                 String             @default("")
  bgColor                       String             @default("")
  changeType                    String?
  from_AnalyticsSection_summary AnalyticsSection[] @relation("AnalyticsSection_summary")
}

model AnalyticsSection {
  id                            String                 @id @default(cuid())
  heading                       String                 @default("")
  subheading                    String                 @default("")
  stats                         AnalyticsStat?         @relation("AnalyticsSection_stats", fields: [statsId], references: [id])
  statsId                       String?                @map("stats")
  tableHeadings                 String                 @default("[]")
  summary                       AnalyticsSummaryItem[] @relation("AnalyticsSection_summary")
  from_Section_contentAnalytics Section[]              @relation("Section_contentAnalytics")

  @@index([statsId])
}

model Value {
  id                String  @id @default(cuid())
  label             String  @default("")
  description       String  @default("")
  icon              String  @default("")
  from_About_values About[] @relation("About_values")
}

model About {
  id                        String    @id @default(cuid())
  heading                   String    @default("")
  intro                     String    @default("")
  valuesTitle               String    @default("")
  values                    Value[]   @relation("About_values")
  closing                   String    @default("")
  from_Section_contentAbout Section[] @relation("Section_contentAbout")
}

model Map {
  id                      String    @id @default(cuid())
  title                   String    @default("")
  subheading              String    @default("")
  description             String    @default("")
  from_Section_contentMap Section[] @relation("Section_contentMap")
}

model Section {
  id                        String              @id @default(cuid())
  type                      String
  contentHero               Hero?               @relation("Section_contentHero", fields: [contentHeroId], references: [id])
  contentHeroId             String?             @map("contentHero")
  contentBenefits           Benefit[]           @relation("Section_contentBenefits")
  contentFeatures           Feature[]           @relation("Section_contentFeatures")
  contentFaqs               Faq[]               @relation("Section_contentFaqs")
  contentTestimonials       TestimonialSection? @relation("Section_contentTestimonials", fields: [contentTestimonialsId], references: [id])
  contentTestimonialsId     String?             @map("contentTestimonials")
  contentCertifications     Certification[]     @relation("Section_contentCertifications")
  contentApproach           Approach[]          @relation("Section_contentApproach")
  contentAbout              About?              @relation("Section_contentAbout", fields: [contentAboutId], references: [id])
  contentAboutId            String?             @map("contentAbout")
  contentAnalytics          AnalyticsSection?   @relation("Section_contentAnalytics", fields: [contentAnalyticsId], references: [id])
  contentAnalyticsId        String?             @map("contentAnalytics")
  contentNavigation         NavigationLink[]    @relation("Section_contentNavigation")
  contentFooter             Footer[]            @relation("Section_contentFooter")
  contentCta                CtaSection[]        @relation("Section_contentCta")
  contentMap                Map?                @relation("Section_contentMap", fields: [contentMapId], references: [id])
  contentMapId              String?             @map("contentMap")
  from_PageContent_sections PageContent[]       @relation("PageContent_sections")

  @@index([contentHeroId])
  @@index([contentTestimonialsId])
  @@index([contentAboutId])
  @@index([contentAnalyticsId])
  @@index([contentMapId])
}

model PageContent {
  id          String    @id @default(cuid())
  title       String    @default("")
  description String    @default("")
  image       Image?    @relation("PageContent_image", fields: [imageId], references: [id])
  imageId     String?   @map("image")
  cta         Cta?      @relation("PageContent_cta", fields: [ctaId], references: [id])
  ctaId       String?   @map("cta")
  sections    Section[] @relation("PageContent_sections")

  @@index([imageId])
  @@index([ctaId])
}
