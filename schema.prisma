datasource sqlite {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "sqlite"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int       @id @default(autoincrement())
  name      String    @default("")
  email     String    @unique @default("")
  password  String
  createdAt DateTime? @default(now())
}

model Type {
  id              Int     @id @default(autoincrement())
  label           String?
  from_Cta_type   Cta[]   @relation("Cta_type")
  from_Image_type Image[] @relation("Image_type")
}

model Cta {
  id                            Int                    @id @default(autoincrement())
  label                         String                 @default("")
  href                          String                 @default("")
  external                      Boolean                @default(false)
  type                          Type?                  @relation("Cta_type", fields: [typeId], references: [id])
  typeId                        Int?                   @map("type")
  from_CertificationSection_cta CertificationSection[] @relation("CertificationSection_cta")
  from_Navigation_cta           Navigation[]           @relation("Navigation_cta")
  from_CtaSection_ctas          CtaSection[]           @relation("CtaSection_ctas")
  from_PageContent_cta          PageContent[]          @relation("PageContent_cta")

  @@index([typeId])
}

model Image {
  id                         Int               @id @default(autoincrement())
  src                        String            @default("")
  alt                        String            @default("")
  width                      Int?
  height                     Int?
  fill                       Boolean           @default(false)
  type                       Type?             @relation("Image_type", fields: [typeId], references: [id])
  typeId                     Int?              @map("type")
  from_Background_image      Background[]      @relation("Background_image")
  from_TestimonialItem_image TestimonialItem[] @relation("TestimonialItem_image")
  from_Certification_image   Certification[]   @relation("Certification_image")
  from_Navigation_image      Navigation[]      @relation("Navigation_image")
  from_PageContent_image     PageContent[]     @relation("PageContent_image")

  @@index([typeId])
}

model Background {
  id                                 Int                  @id @default(autoincrement())
  image                              Image?               @relation("Background_image", fields: [imageId], references: [id])
  imageId                            Int?                 @map("image")
  outerClassName                     String               @default("")
  from_TestimonialSection_background TestimonialSection[] @relation("TestimonialSection_background")
  from_CtaSection_background         CtaSection[]         @relation("CtaSection_background")

  @@index([imageId])
}

model Language {
  id                    Int      @id @default(autoincrement())
  label                 String
  value                 String
  from_Footer_languages Footer[] @relation("Footer_languages")
}

model TestimonialBadge {
  id                         Int               @id @default(autoincrement())
  icon                       String            @default("")
  label                      String            @default("")
  from_TestimonialItem_badge TestimonialItem[] @relation("TestimonialItem_badge")
}

model TestimonialItem {
  id                                   Int                  @id @default(autoincrement())
  rating                               Float?
  badge                                TestimonialBadge?    @relation("TestimonialItem_badge", fields: [badgeId], references: [id])
  badgeId                              Int?                 @map("badge")
  name                                 String               @default("")
  role                                 String               @default("")
  company                              String               @default("")
  image                                Image?               @relation("TestimonialItem_image", fields: [imageId], references: [id])
  imageId                              Int?                 @map("image")
  content                              String               @default("")
  from_TestimonialSection_testimonials TestimonialSection[] @relation("TestimonialSection_testimonials")
  from_TestimonialSection_fallback     TestimonialSection[] @relation("TestimonialSection_fallback")

  @@index([badgeId])
  @@index([imageId])
}

model TestimonialSection {
  id                               Int               @id @default(autoincrement())
  title                            String            @default("")
  background                       Background[]      @relation("TestimonialSection_background")
  testimonials                     TestimonialItem[] @relation("TestimonialSection_testimonials")
  fallback                         TestimonialItem?  @relation("TestimonialSection_fallback", fields: [fallbackId], references: [id])
  fallbackId                       Int?              @map("fallback")
  from_Section_contentTestimonials Section[]         @relation("Section_contentTestimonials")

  @@index([fallbackId])
}

model HeroBannerAdditional {
  id                         Int          @id @default(autoincrement())
  icon                       String       @default("")
  text                       String       @default("")
  from_HeroBanner_additional HeroBanner[] @relation("HeroBanner_additional")
}

model HeroBanner {
  id               Int                   @id @default(autoincrement())
  label            String                @default("")
  href             String                @default("")
  external         Boolean               @default(false)
  icon             String                @default("")
  additional       HeroBannerAdditional? @relation("HeroBanner_additional", fields: [additionalId], references: [id])
  additionalId     Int?                  @map("additional")
  from_Hero_banner Hero[]                @relation("Hero_banner")

  @@index([additionalId])
}

model Hero {
  id                       Int         @id @default(autoincrement())
  title                    String      @default("")
  description              String      @default("")
  subHeading               String      @default("")
  banner                   HeroBanner? @relation("Hero_banner", fields: [bannerId], references: [id])
  bannerId                 Int?        @map("banner")
  from_Section_contentHero Section[]   @relation("Section_contentHero")

  @@index([bannerId])
}

model Benefit {
  id                           Int              @id @default(autoincrement())
  icon                         String           @default("")
