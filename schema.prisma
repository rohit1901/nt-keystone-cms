  from_Section_contentBenefits Section[] @relation("Section_contentBenefits")
}

model Faq {
  id                       Int       @id @default(autoincrement())
  question                 String    @default("")
  answer                   String    @default("")
  from_Section_contentFaqs Section[] @relation("Section_contentFaqs")
}

model Certification {
  id                                       Int                    @id @default(autoincrement())
  title                                    String                 @default("")
  description                              String                 @default("")
  image                                    Image?                 @relation("Certification_image", fields: [imageId], references: [id])
  imageId                                  Int?                   @map("image")
  link                                     String                 @default("")
  from_CertificationSection_certifications CertificationSection[] @relation("CertificationSection_certifications")

  @@index([imageId])
}

model CertificationSection {
  id                                 Int             @id @default(autoincrement())
  title                              String          @default("")
  description                        String          @default("")
  cta                                Cta?            @relation("CertificationSection_cta", fields: [ctaId], references: [id])
  ctaId                              Int?            @map("cta")
  certifications                     Certification[] @relation("CertificationSection_certifications")
  from_Section_contentCertifications Section[]       @relation("Section_contentCertifications")

  @@index([ctaId])
}

model Feature {
  id                           Int       @id @default(autoincrement())
