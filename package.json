{
  "name": "keystone-app",
  "version": "1.0.3",
  "private": true,
  "scripts": {
    "dev": "keystone dev",
    "dev:preinstall": "cp .env.copy .env.dev && npm install",
    "build": "keystone build",
    "start": "keystone start --with-migrations",
    "generate": "keystone prisma migrate dev",
    "db:seed": "ts-node --transpile-only ./seed/index.ts",
    "db:reset": "keystone prisma db push --force-reset",
    "db:reset:seed": "npm run db:reset && npm run db:seed"
  },
  "dependencies": {
    "@keystone-6/auth": "^8.0.0",
    "@keystone-6/core": "^6.0.0",
    "@keystone-6/fields-document": "^9.0.0",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-separator": "1.0.3",
    "@radix-ui/react-slot": "1.0.2",
    "class-variance-authority": "^0.7.1",
    "dotenv": "^17.2.3",
    "next-auth": "^4.24.11",
    "tailwind-merge": "^2.3.0",
    "typescript": "^5.5.0"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.11",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0"
  }
}
