{
  "name": "keystone-app",
  "version": "1.0.3",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development keystone dev",
    "build": "keystone build",
    "seed": "ts-node --transpile-only seed.ts",
    "generate": "keystone prisma migrate dev",
    "start": "keystone start --with-migrations",
    "db:reset": "keystone prisma db push --force-reset",
    "db:reset:seed": "npm run db:reset && npm run seed"
  },
  "dependencies": {
    "@keystone-6/auth": "^8.0.0",
    "@keystone-6/core": "^6.0.0",
    "@keystone-6/fields-document": "^9.0.0",
    "typescript": "^5.5.0"
  },
  "devDependencies": {
    "dotenv": "^17.2.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0"
  }
}
